    "ssr:build": "npm run client:build && npm run server:build",
    "client:build": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config ./webpack.client.js --progress --hide-modules",
    "server:build": "./node_modules/.bin/cross-env NODE_ENV=production webpack --config ./webpack.server.js --progress --hide-modules",
    "start-server": "node server.js"
  },
  "lint-staged": {
